<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="false" debug="false">
    <include resource="org/springframework/boot/logging/logback/defaults.xml"/>
    <include resource="org/springframework/boot/logging/logback/console-appender.xml"/>

    <root level="WARN">
        <appender-ref ref="CONSOLE"/>
    </root>

    <logger name="no.nav.foerstesidegenerator" level="INFO"/>

    <!--<logger name="no.nav.security.oidc" level="DEBUG"/>-->
    <!--<logger name="no.nav.security.spring" level="DEBUG"/>-->
    <!--<logger name="org.springframework.security" level="DEBUG"/>-->
    <logger name="org.apache.cxf.common.logging" level="WARN" additivity="false"/>

    <logger name="org.apache.cxf.interceptor" additivity="true">
        <appender-ref ref="CONSOLE"/>
    </logger>

</configuration>